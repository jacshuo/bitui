    "vite": "^2.8.0",
    "vite-dts": "^1.0.4",
    "vite-plugin-html": "^3.0.6",
    "vite-tsconfig-paths": "^3.4.0"
  },
  "files": [
    "dist",
    "src/lib/common",
    "src/lib/components",
    "src/lib/index.ts",
